    "@tryghost/content-api": "^1.4.13",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-sass": "1.0.1",
    "chart.js": "2.9.3",
    "cypress": "^6.3.0",
    "cypress-react-unit-test": "^4.17.2",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "next": "10.0.5",
    "next-compose-plugins": "2.2.0",
    "next-fonts": "1.4.0",
    "next-images": "1.3.1",
    "node-sass": "4.14.1",
    "path": "0.12.7",
    "react": "16.13.1",
    "react-dom": "16.13.1",
